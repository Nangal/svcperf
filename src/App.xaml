<Application x:Class="EtlViewer.App"            
             x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Viewer\MainWindow.xaml"  Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Brushes.xaml" />
                <ResourceDictionary Source="Assets/Fonts.xaml" />
                <ResourceDictionary Source="Assets/MenuStyles.xaml" />
                <ResourceDictionary Source="Assets/MetroExpander.xaml" />
                <ResourceDictionary Source="Assets/ControlStyles.xaml" />
                <ResourceDictionary Source="Assets/StatusBarStyles.xaml" />
                <ResourceDictionary Source="Assets/SequenceDiagramStyle.xaml" />
                <ResourceDictionary Source="themes/generic.xaml" />
                <ResourceDictionary Source="Assets/WindowStyles.xaml" />                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
